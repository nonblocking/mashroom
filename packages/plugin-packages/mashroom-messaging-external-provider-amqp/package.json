{
    "name": "@mashroom/mashroom-messaging-external-provider-amqp",
    "description": "Mashroom plugin that can use an AMQP 1.0 compliant broker as external messaging provider for service side messaging",
    "homepage": "https://www.mashroom-server.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "license": "MIT",
    "version": "2.9.3",
    "files": [
        "dist/**",
        "mashroom.json"
    ],
    "dependencies": {
        "@mashroom/mashroom-utils": "2.9.3",
        "rhea": "^3.0.4"
    },
    "devDependencies": {
        "@mashroom/mashroom": "2.9.3",
        "@mashroom/mashroom-messaging": "2.9.3",
        "@mashroom/mashroom-monitoring-metrics-collector": "2.9.3"
    },
    "jest": {
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/test"
        ],
        "testRegex": "(\\.(test|spec))\\.ts",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "reporters": [
            "default",
            "jest-junit"
        ]
    },
    "jest-junit": {
        "outputDirectory": "./test-reports"
    },
    "scripts": {
        "lint": "eslint --fix",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "build": "babel src -d dist --extensions \".ts\""
    }
}
