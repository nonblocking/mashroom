        "mashroom.json"
    ],
    "devDependencies": {
        "@mashroom/mashroom-portal": "2.9.3",
        "@types/react": "^19.1.3",
        "@types/react-dom": "^19.1.4",
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
    },
    "scripts": {
