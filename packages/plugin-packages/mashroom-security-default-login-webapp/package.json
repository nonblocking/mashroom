{
    "name": "@mashroom/mashroom-security-default-login-webapp",
    "description": "The default login webapp, that will be available at /login",
    "homepage": "https://www.mashroom-server.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "license": "MIT",
    "version": "2.9.4",
    "files": [
        "dist/**",
        "assets/**",
        "views/**",
        "mashroom.json"
    ],
    "dependencies": {
        "body-parser": "^2.2.1",
        "express-handlebars": "^7.1.3",
        "handlebars": "^4.7.8"
    },
    "devDependencies": {
        "@mashroom/mashroom": "2.9.4",
        "@mashroom/mashroom-browser-cache": "2.9.4",
        "@mashroom/mashroom-csrf-protection": "2.9.4",
        "@mashroom/mashroom-i18n": "2.9.4",
        "@mashroom/mashroom-security": "2.9.4",
        "@mashroom/mashroom-vhost-path-mapper": "2.9.4",
        "@types/express-session": "^1.18.1"
    },
    "scripts": {
        "lint": "eslint --fix",
        "type-check": "tsc --noEmit",
        "dev": "nodemon --watch src -e ts,scss --exec \"npm run build && node dist/dev-server.js\"",
        "build": "babel src/js --extensions \".ts\" -d dist && sass --style compressed --no-source-map src/sass/style.scss dist/style.css"
    }
}
