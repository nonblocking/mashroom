{
    "name": "@mashroom/mashroom-portal-demo-angular-app",
    "description": "Simple Angular 8 based app that integrates into the Portal",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.5.4",
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1002.0",
        "@angular/cli": "^10.2.0",
        "@angular/common": "^10.2.2",
        "@angular/compiler": "^10.2.2",
        "@angular/compiler-cli": "^10.2.2",
        "@angular/core": "^10.2.2",
        "@angular/forms": "^10.2.2",
        "@angular/language-service": "^10.2.2",
        "@angular/platform-browser": "^10.2.2",
        "@angular/platform-browser-dynamic": "^10.2.2",
        "@mashroom/mashroom-portal": "1.5.4",
        "@types/node": "^14.14.6",
        "core-js": "^3.7.0",
        "rxjs": "^6.6.3",
        "tslint": "^6.1.3",
        "typescript": "4.0.5",
        "zone.js": "^0.11.3"
    },
    "scripts": {
        "dev": "ng serve",
        "build": "ng build --prod=true",
        "lint": "ng lint",
        "type-check": "tsc --noEmit"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Angular App",
                "title": "Demo Angular App",
                "type": "portal-app",
                "bootstrap": "startAngularDemoApp",
                "category": "Demo",
                "sharedResources": {
                    "js": [
                        "zonejs.js"
                    ]
                },
                "resources": {
                    "js": [
                        "runtime.js",
                        "main.js"
                    ],
                    "css": [
                        "styles.css"
                    ]
                },
                "defaultConfig": {
                    "resourcesRoot": "./dist",
                    "restProxies": {},
                    "appConfig": {
                        "firstName": "Kim"
                    }
                }
            }
        ]
    }
}
