{
    "name": "@mashroom/mashroom-portal-demo-angular-app",
    "description": "A simple Angular based SPA that can be used in the Mashroom Portal",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.9.1",
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@mashroom/mashroom-portal": "1.9.1",
        "@angular-devkit/build-angular": "^13.0.3",
        "@angular/cli": "^13.0.3",
        "@angular/common": "^13.0.2",
        "@angular/compiler": "^13.0.2",
        "@angular/compiler-cli": "^13.0.2",
        "@angular/core": "^13.0.2",
        "@angular/forms": "^13.0.2",
        "@angular/language-service": "^13.0.2",
        "@angular/platform-browser": "^13.0.2",
        "@angular/platform-browser-dynamic": "^13.0.2",
        "@angular-eslint/schematics": "^13.0.1",
        "@angular-eslint/builder": "^13.0.1",
        "@angular-eslint/template-parser": "^13.0.1",
        "eslint": "^8.3.0",
        "@types/node": "^16.11.10",
        "rxjs": "^7.4.0",
        "typescript": "4.4.4",
        "zone.js": "^0.11.4"
    },
    "scripts": {
        "dev": "ng serve",
        "build": "ng build --configuration production",
        "lint": "ng lint",
        "type-check": "tsc --noEmit"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Angular App",
                "title": "Demo Angular App",
                "type": "portal-app",
                "clientBootstrap": "startAngularDemoApp",
                "category": "Demo",
                "resources": {
                    "js": [
                        "runtime.js",
                        "main.js"
                    ],
                    "css": [
                        "styles.css"
                    ]
                },
                "local": {
                    "resourcesRoot": "./dist"
                },
                "defaultConfig": {
                    "appConfig": {
                        "firstName": "Kim",
                        "pingButtonLabel": "Send Ping"
                    }
                }
            }
        ]
    }
}
