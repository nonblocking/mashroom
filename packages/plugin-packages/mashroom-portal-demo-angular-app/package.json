{
    "name": "@mashroom/mashroom-portal-demo-angular-app",
    "description": "A simple Angular based SPA that can be used in the Mashroom Portal",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.9.2",
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.2.6",
        "@angular/cli": "^12.2.6",
        "@angular/common": "^12.2.6",
        "@angular/compiler": "^12.2.6",
        "@angular/compiler-cli": "^12.2.6",
        "@angular/core": "^12.2.6",
        "@angular/forms": "^12.2.6",
        "@angular/language-service": "^12.2.6",
        "@angular/platform-browser": "^12.2.6",
        "@angular/platform-browser-dynamic": "^12.2.6",
        "@mashroom/mashroom-portal": "1.9.2",
        "@types/node": "^14.17.17",
        "rxjs": "^7.3.0",
        "tslint": "^6.1.3",
        "typescript": "4.3.5",
        "zone.js": "^0.11.4"
    },
    "scripts": {
        "dev": "ng serve",
        "build": "ng build --configuration production",
        "lint": "ng lint",
        "type-check": "tsc --noEmit"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Angular App",
                "title": "Demo Angular App",
                "type": "portal-app",
                "bootstrap": "startAngularDemoApp",
                "category": "Demo",
                "resources": {
                    "js": [
                        "runtime.js",
                        "main.js"
                    ],
                    "css": [
                        "styles.css"
                    ]
                },
                "defaultConfig": {
                    "resourcesRoot": "./dist",
                    "restProxies": {},
                    "appConfig": {
                        "firstName": "Kim",
                        "pingButtonLabel": "Send Ping"
                    }
                }
            }
        ]
    }
}
