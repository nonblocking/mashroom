{
    "name": "@mashroom/mashroom-session-provider-filestore",
    "description": "Mashroom Server session file store provider",
    "homepage": "https://www.mashroom-server.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "license": "MIT",
    "version": "2.8.1",
    "files": [
        "dist/**"
    ],
    "dependencies": {
        "express": "^4.19.2",
        "session-file-store": "^1.5.0"
    },
    "devDependencies": {
        "@mashroom/mashroom": "2.8.1",
        "@mashroom/mashroom-session": "2.8.1",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.1",
        "@types/session-file-store": "^1.2.5"
    },
    "scripts": {
        "lint": "eslint --fix",
        "type-check": "tsc --noEmit",
        "build": "babel src -d dist --extensions \".ts\""
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Session Filestore Provider",
                "type": "session-store-provider",
                "bootstrap": "./dist/mashroom-bootstrap.js",
                "defaultConfig": {
                    "path": "./data/sessions"
                }
            }
        ]
    }
}
