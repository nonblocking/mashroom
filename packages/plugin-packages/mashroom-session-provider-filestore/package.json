{
    "name": "@mashroom/mashroom-session-provider-filestore",
    "description": "Mashroom Server session file store provider",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.5.4",
    "files": [
        "dist/**"
    ],
    "dependencies": {
        "session-file-store": "^1.4.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@mashroom/mashroom": "1.5.4",
        "@mashroom/mashroom-session": "1.5.4",
        "@types/express": "^4.17.6",
        "@types/express-session": "^1.17.0",
        "@types/session-file-store": "^1.2.1",
        "eslint": "^6.8.0",
        "express": "^4.17.1",
        "typescript": "^3.8.3"
    },
    "scripts": {
        "lint": "eslint  src --ext '.ts' --fix",
        "type-check": "tsc --noEmit",
        "build": "babel src -d dist --extensions '.ts'"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Session Filestore Provider",
                "type": "session-store-provider",
                "bootstrap": "./dist/mashroom-bootstrap.js",
                "defaultConfig": {
                    "path": "./data/sessions"
                }
            }
        ]
    }
}
