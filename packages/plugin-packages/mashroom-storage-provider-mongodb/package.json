{
    "name": "@mashroom/mashroom-storage-provider-mongodb",
    "description": "Mashroom storage provider backed by MongoDB",
    "homepage": "https://www.mashroom-server.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "license": "MIT",
    "version": "2.9.2",
    "files": [
        "dist/**",
        "mashroom.json"
    ],
    "dependencies": {
        "mongodb": "^6.16.0"
    },
    "devDependencies": {
        "@mashroom/mashroom": "2.9.2",
        "@mashroom/mashroom-monitoring-metrics-collector": "2.9.2",
        "@mashroom/mashroom-storage": "2.9.2",
        "@mashroom/mashroom-utils": "2.9.2",
        "@shelf/jest-mongodb": "^5.1.0"
    },
    "jest": {
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/test"
        ],
        "testRegex": "(\\.(test|spec))\\.ts",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "preset": "@shelf/jest-mongodb",
        "reporters": [
            "default",
            "jest-junit"
        ]
    },
    "jest-junit": {
        "outputDirectory": "./test-reports"
    },
    "scripts": {
        "lint": "eslint --fix",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "build": "babel src -d dist --extensions \".ts\""
    },
    "lavamoat": {
        "allowScripts": {
            "@shelf/jest-mongodb>mongodb-memory-server": false
        }
    }
}
