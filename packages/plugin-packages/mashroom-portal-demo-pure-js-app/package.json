{
    "name": "@mashroom/mashroom-portal-demo-pure-js-app",
    "description": "A pure JS Microfrontend that uses ES modules",
    "homepage": "https://www.mashroom-server.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "license": "MIT",
    "version": "1.0.0",
    "sideEffects": false,
    "files": [
        "dist/**",
        "mashroom.ts"
    ],
    "devDependencies": {
        "@mashroom/mashroom-portal": "2.9.4"
    },
    "scripts": {
        "lint": "eslint --fix",
        "type-check": "tsc --noEmit",
        "dev": "nodemon --watch src --ext ts,scss --exec \"npm run build && http-server . -p 5111 -c-1 -o /testPage.html\"",
        "build": "tsc && sass src/style.scss dist/style.css"
    }
}
