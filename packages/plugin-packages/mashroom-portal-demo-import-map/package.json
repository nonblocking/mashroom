{
    "name": "@mashroom/mashroom-portal-demo-import-map",
    "description": "Demo Microfrontends that shares external vendor modules via import map",
    "homepage": "https://www.mashroom-server.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "license": "MIT",
    "version": "1.0.0",
    "sideEffects": false,
    "files": [
        "dist/**",
        "mashroom.ts"
    ],
    "devDependencies": {
        "@mashroom/mashroom-portal": "2.9.3"

    },
    "scripts": {
        "lint": "eslint --fix",
        "type-check": "tsc --noEmit",
        "dev:app1": "webpack serve --config ./webpack.config.app1.js --mode development",
        "dev:app2": "webpack serve --config ./webpack.config.app2.js --mode development",
        "build": "webpack --config ./webpack.config.app1.js --mode production && webpack --config ./webpack.config.app2.js --mode production"
    }
}
