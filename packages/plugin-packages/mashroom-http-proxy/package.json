{
    "name": "@mashroom/mashroom-http-proxy",
    "description": "Mashroom http proxy service",
    "homepage": "https://www.mashroom-server.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "license": "MIT",
    "version": "2.9.2",
    "files": [
        "dist/**",
        "type-definitions/*",
        "mashroom.json"
    ],
    "dependencies": {
        "http-proxy": "^1.18.1"
    },
    "devDependencies": {
        "@mashroom/mashroom": "2.9.2",
        "@mashroom/mashroom-monitoring-metrics-collector": "2.9.2",
        "@mashroom/mashroom-security": "2.9.2",
        "@mashroom/mashroom-utils": "2.9.2",
        "@types/http-proxy": "1.17.9",
        "@types/ws": "^8.18.1",
        "ws": "^8.18.2"
    },
    "jest": {
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/test"
        ],
        "testRegex": "(\\.(test|spec))\\.ts",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "reporters": [
            "default",
            "jest-junit"
        ]
    },
    "jest-junit": {
        "outputDirectory": "./test-reports"
    },
    "scripts": {
        "lint": "eslint --fix",
        "type-check": "tsc --noEmit",
        "test": "jest --runInBand",
        "build": "babel src -d dist --extensions \".ts\""
    }
}
