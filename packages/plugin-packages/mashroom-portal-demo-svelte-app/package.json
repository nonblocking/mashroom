{
    "name": "@mashroom/mashroom-portal-demo-svelte-app",
    "version": "1.6.4",
    "description": "Simple Svelte based app that integrates into the Portal",
    "homepage": "https://www.mashroom-server.com",
    "license": "MIT",
    "main": "index.js",
    "sideEffects": false,
    "files": [
        "public/**"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "dev": "webpack serve",
        "type-check": "svelte-check"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "keywords": [
        "mashroom",
        "svelte"
    ],
    "author": "Milan Heimschild",
    "bugs": {
        "url": "https://github.com/nonblocking/mashroom/issues"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.16",
        "@mashroom/mashroom": "^1.6.4",
        "@mashroom/mashroom-portal": "^1.6.4",
        "@tsconfig/svelte": "^1.0.10",
        "cross-env": "^7.0.3",
        "svelte": "^3.32.3",
        "svelte-check": "^1.1.35",
        "svelte-loader": "^3.0.0",
        "svelte-preprocess": "^4.6.9",
        "typescript": "^4.1.5",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Svelte App",
                "title": "Demo Svelte App",
                "type": "portal-app",
                "bootstrap": "startSvelteDemoApp",
                "category": "Demo",
                "resources": {
                    "js": [
                        "bundle.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./public",
                    "restProxies": {},
                    "appConfig": {
                        "firstName": "Gus"
                    }
                }
            }
        ]
    }
}
