{
    "name": "@mashroom/mashroom-portal-demo-svelte-app",
    "version": "1.9.1",
    "description": "A simple Svelte based SPA that can be used in the Mashroom Portal",
    "homepage": "https://www.mashroom-server.com",
    "license": "MIT",
    "main": "index.js",
    "sideEffects": false,
    "files": [
        "public/**"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "dev": "webpack serve",
        "type-check": "svelte-check"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "keywords": [
        "mashroom",
        "svelte"
    ],
    "author": "Milan Heimschild",
    "bugs": {
        "url": "https://github.com/nonblocking/mashroom/issues"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@mashroom/mashroom": "1.9.1",
        "@mashroom/mashroom-portal": "1.9.1",
        "@tsconfig/svelte": "^2.0.1",
        "cross-env": "^7.0.3",
        "svelte": "^3.42.6",
        "svelte-check": "^2.2.6",
        "svelte-loader": "^3.1.2",
        "svelte-preprocess": "^4.9.4",
        "typescript": "^4.4.3",
        "webpack": "^5.53.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.2.1"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Svelte App",
                "title": "Demo Svelte App",
                "type": "portal-app",
                "bootstrap": "startSvelteDemoApp",
                "category": "Demo",
                "resources": {
                    "js": [
                        "bundle.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./public",
                    "restProxies": {},
                    "appConfig": {
                        "firstName": "Gus",
                        "pingButtonLabel": "Send Ping"
                    }
                }
            }
        ]
    }
}
