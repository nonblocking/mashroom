{
    "name": "@mashroom/mashroom-portal-demo-svelte-app",
    "version": "1.5.4",
    "description": "Simple Svelte based app that integrates into the Portal",
    "homepage": "https://www.mashroom-server.com",
    "license": "MIT",
    "main": "index.js",
    "sideEffects": false,
    "files": [
        "public/**"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "dev": "webpack serve"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "keywords": [
        "mashroom",
        "svelte"
    ],
    "author": "Milan Heimschild",
    "bugs": {
        "url": "https://github.com/nonblocking/mashroom/issues"
    },
    "devDependencies": {
        "@mashroom/mashroom-portal": "1.5.4",
        "@babel/cli": "^7.12.1",
        "cross-env": "^7.0.2",
        "svelte": "^3.29.4",
        "svelte-loader": "^2.13.6",
        "webpack": "^4.44.2",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Svelte App",
                "title": "Demo Svelte App",
                "type": "portal-app",
                "bootstrap": "startSvelteDemoApp",
                "category": "Demo",
                "resources": {
                    "js": [
                        "bundle.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./public",
                    "restProxies": {},
                    "appConfig": {
                        "firstName": "Gus"
                    }
                }
            }
        ]
    }
}
