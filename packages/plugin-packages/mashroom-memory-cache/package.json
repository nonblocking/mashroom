{
    "name": "@mashroom/mashroom-memory-cache",
    "description": "A memory cache service with pluggable providers",
    "homepage": "https://www.mashroom-server.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nonblocking/mashroom.git"
    },
    "license": "MIT",
    "version": "2.8.1",
    "files": [
        "dist/**",
        "type-definitions/*"
    ],
    "dependencies": {
        "express": "^4.21.2",
        "node-cache": "^5.1.2"
    },
    "devDependencies": {
        "@mashroom/mashroom": "2.8.1",
        "@mashroom/mashroom-monitoring-metrics-collector": "2.8.1",
        "@types/express": "^4.17.21"
    },
    "jest": {
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/test"
        ],
        "testRegex": "(\\.(test|spec))\\.ts",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "reporters": [
            "default",
            "jest-junit"
        ]
    },
    "jest-junit": {
        "outputDirectory": "./test-reports"
    },
    "scripts": {
        "lint": "eslint --fix",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "build": "babel src -d dist --extensions \".ts\""
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Memory Cache Services",
                "type": "services",
                "namespace": "memorycache",
                "bootstrap": "./dist/services/mashroom-bootstrap-services.js",
                "defaultConfig": {
                    "provider": "local",
                    "defaultTTLSec": 300
                }
            },
            {
                "name": "Mashroom Memory Cache Provider Loader",
                "type": "plugin-loader",
                "bootstrap": "./dist/plugins/loader/mashroom-bootstrap-plugin-loader.js",
                "loads": "memory-cache-provider"
            }
        ]
    }
}
