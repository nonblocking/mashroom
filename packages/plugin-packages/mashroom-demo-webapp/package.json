{
    "name": "@mashroom/mashroom-demo-webapp",
    "description": "Demo express webapp that will be available at /demo/webapp",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.6.4",
    "files": [
        "dist/**"
    ],
    "dependencies": {
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@mashroom/mashroom": "1.6.4",
        "eslint": "^7.13.0",
        "nodemon": "^2.0.6",
        "typescript": "^4.1.5",
        "@types/express": "^4.17.11"
    },
    "scripts": {
        "lint": "eslint src --ext \".ts\" --fix",
        "type-check": "tsc --noEmit",
        "build": "babel src -d dist --extensions \".ts\"",
        "dev": "nodemon --watch src --exec \"npm run build && node dist/dev_server.js\""
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Demo Webapp",
                "type": "web-app",
                "bootstrap": "./dist/mashroom-bootstrap.js",
                "defaultConfig": {
                    "path": "/demo/webapp"
                }
            }
        ]
    }
}
