{
    "name": "@mashroom/mashroom-portal-demo-load-dynamically-app",
    "description": "A simple SPA that demonstrates how an App registered to the _Mashroom Portal_ can load and unload other Portal Apps on a page with a specific config",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.9.2",
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@mashroom/mashroom-portal": "1.9.2",
        "@types/react": "^17.0.21",
        "@types/react-dom": "^17.0.9",
        "eslint": "^7.32.0",
        "jest": "^27.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "typescript": "^4.4.3",
        "webpack": "^5.53.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.2.1"
    },
    "scripts": {
        "lint": "eslint --ext .ts,.tsx --fix --quiet ./src",
        "type-check": "tsc --noEmit",
        "dev": "webpack serve --hot --mode development",
        "build": "webpack --mode production"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Load Dynamically App",
                "title": {
                    "en": "Demo Load Dynamically",
                    "de": "Demo Dynamisches Laden App"
                },
                "type": "portal-app",
                "bootstrap": "startLoadDynamicallyDemoApp",
                "requires": [
                    "Mashroom Portal Demo React App",
                    "Mashroom Portal Demo Angular App"
                ],
                "category": "Demo",
                "resources": {
                    "js": [
                        "bundle.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./dist"
                }
            }
        ]
    }
}
