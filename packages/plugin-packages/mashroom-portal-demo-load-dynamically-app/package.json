{
    "name": "@mashroom/mashroom-portal-demo-load-dynamically-app",
    "description": "Mashroom Portal Demo Load apps dynamically",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.8.0",
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@mashroom/mashroom-portal": "1.8.0",
        "@types/react": "^17.0.15",
        "@types/react-dom": "^17.0.9",
        "eslint": "^7.32.0",
        "jest": "^27.0.6",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "typescript": "^4.3.5",
        "webpack": "^5.48.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2"
    },
    "scripts": {
        "lint": "eslint --ext .ts,.tsx --fix --quiet ./src",
        "type-check": "tsc --noEmit",
        "dev": "webpack serve --hot --mode development",
        "build": "webpack --mode production"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Load Dynamically App",
                "title": {
                    "en": "Demo Load Dynamically",
                    "de": "Demo dynamisches Laden App"
                },
                "type": "portal-app",
                "bootstrap": "startLoadDynamicallyDemoApp",
                "requires": [
                    "Mashroom Portal Demo React App",
                    "Mashroom Portal Demo Angular App"
                ],
                "category": "Demo",
                "resources": {
                    "js": [
                        "bundle.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./dist"
                }
            }
        ]
    }
}
