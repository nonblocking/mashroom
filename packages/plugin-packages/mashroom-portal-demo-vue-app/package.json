{
    "name": "@mashroom/mashroom-portal-demo-vue-app",
    "description": "Simple Vue based app that integrates into the Portal",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.6.4",
    "sideEffects": false,
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@vue/cli-plugin-eslint": "^4.5.8",
        "copyfiles": "^2.4.0",
        "eslint": "^7.13.0",
        "eslint-plugin-vue": "^7.1.0",
        "jest": "^26.6.3",
        "vue": "^2.6.12",
        "vue-eslint-parser": "^7.1.1",
        "vue-loader": "^15.9.5",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^4.44.2",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "scripts": {
        "lint": "eslint src --fix",
        "copy-assets": "copyfiles -u 1 src/assets/* dist",
        "dev": "webpack serve --hot --mode development",
        "build": "webpack --mode production && npm run copy-assets"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Vue App",
                "title": "Demo Vue App",
                "type": "portal-app",
                "bootstrap": "startVueDemoApp",
                "category": "Demo",
                "resources": {
                    "js": [
                        "bundle.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./dist",
                    "restProxies": {},
                    "appConfig": {
                        "firstName": "Mike"
                    }
                }
            }
        ]
    }
}
