{
    "$schema": "../../core/mashroom-json-schemas/schemas/mashroom-security-acl.json",
    "/mashroom/**": {
        "*": {
            "allow": {
                "roles": ["Administrator"],
                "ips": ["127.0.0.1", "::1", "::ffff:127.0.0.1"]
            }
        }
    },
    "/portal/public-site/**": {
        "*": {
            "allow": "any"
        }
    },
    "/portal/**": {
        "*": {
            "allow": {
                "roles": ["Authenticated"]
            }
        }
    },
    "/websocket/test/**": {
        "*": {
            "allow": {
                "roles": ["Authenticated"]
            }
        }
    }
}
